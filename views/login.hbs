<h1>Login</h1>
{{#if failedToLogin}}
	<p>Wrong credentials!</p>
{{/if}}

<form id="all-forms" action="" method="post">
    <div>
    Username: <input type="text" name="username">
    </div>
    <div>
        Password: <input type="password" name="password">
    </div>
    <input type="submit" value="Login"  id="submit-button">
</form>